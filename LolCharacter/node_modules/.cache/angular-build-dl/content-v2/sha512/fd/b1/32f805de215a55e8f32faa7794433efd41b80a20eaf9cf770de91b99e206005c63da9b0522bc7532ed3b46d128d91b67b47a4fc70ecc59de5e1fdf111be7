let SortsComponent = class SortsComponent {
    constructor(championService, sortsService, route, router) {
        this.championService = championService;
        this.sortsService = sortsService;
        this.route = route;
        this.router = router;
    }
    ngOnInit() {
        if (!sessionStorage.getItem("isLoginAs")) {
            this.router.navigate(['/connexion']);
        }
        this.championService.getById(this.route.snapshot.params.id).subscribe(champion => {
            this.champion = champion;
        });
        this.sortsService.getWithChampId(this.route.snapshot.params.id)
            .subscribe(sorts => {
            this.sorts = sorts;
        });
    }
    delete(id) {
        this.sortsService.delete(id).subscribe(res => {
            this.ngOnInit();
        });
    }
};
