        this.http = http;
    }
    getWithUserId(id) {
        return this.http.get("http://localhost:4200/champions?id_user=" + id);
    }
    getById(id) {
        return this.http.get("http://localhost:4200/champions/" + id);
    }
    post(champion) {
        return this.http.post("http://localhost:4200/champions", champion);
    }
    delete(id) {
        return this.http.delete('http://localhost:4200/champions/' + id);
    }
    put(id, champion) {
        return this.http.put('http://localhost:4200/champions/' + id, champion);
    }
