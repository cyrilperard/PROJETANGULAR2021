let SortsService = class SortsService {
    constructor(http) {
        this.http = http;
    }
    post(sort) {
        return this.http.post("http://localhost:4200/sorts", sort);
    }
    getWithChampId(id) {
        return this.http.get("http://localhost:4200/sorts?id_champion=" + id);
    }
    delete(id) {
        return this.http.delete('http://localhost:4200/sorts/' + id);
    }
    getById(id) {
        return this.http.get("http://localhost:4200/sorts/" + id);
    }
    put(id, sort) {
        return this.http.put('http://localhost:4200/sorts/' + id, sort);
    }
};
