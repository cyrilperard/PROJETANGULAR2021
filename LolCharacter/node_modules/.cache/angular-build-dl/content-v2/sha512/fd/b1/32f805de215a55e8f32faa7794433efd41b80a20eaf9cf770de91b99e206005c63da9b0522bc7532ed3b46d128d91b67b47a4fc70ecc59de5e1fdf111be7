let DialogOverviewCreerDialog = class DialogOverviewCreerDialog {
    constructor(dialogRef, formBuilder, service, route) {
        this.dialogRef = dialogRef;
        this.formBuilder = formBuilder;
        this.service = service;
        this.route = route;
    }
    ngOnInit() {
        this.registerForm = this.formBuilder.group({
            // id: ['', Validators.required],
            nom: ['', _angular_forms__WEBPACK_IMPORTED_MODULE_2__["Validators"].required],
            lv: ['', _angular_forms__WEBPACK_IMPORTED_MODULE_2__["Validators"].required],
            hp: ['', _angular_forms__WEBPACK_IMPORTED_MODULE_2__["Validators"].required],
            ap: ['', _angular_forms__WEBPACK_IMPORTED_MODULE_2__["Validators"].required],
        });
    }
    get f() { return this.registerForm.controls; }
    onSubmit() {
        this.submitted = true;
        if (this.registerForm.invalid) {
            return;
        }
        new_champion: src_shared_model_champions__WEBPACK_IMPORTED_MODULE_3__["Champions"];
        let new_champion = new src_shared_model_champions__WEBPACK_IMPORTED_MODULE_3__["Champions"]();
        // new_champion.id = this.registerForm.value.id;
        new_champion.nom = this.registerForm.value.nom;
        new_champion.lv = this.registerForm.value.lv;
        new_champion.hp = this.registerForm.value.hp;
        new_champion.ap = this.registerForm.value.ap;
        new_champion.id_user = sessionStorage.getItem("isLoginAs");
        this.service.post(new_champion)
            .subscribe({ next: res => {
                console.log(res);
                this.route.navigate(['/champion']).then(() => this.route.navigate(['/champions']));
            }
        });
        this.onNoClick();
    }
    onNoClick() {
        this.dialogRef.close();
    }
};
